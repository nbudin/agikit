{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch Extract",
      "skipFiles": ["<node_internals>/**"],
      "cwd": "${workspaceFolder}",
      "runtimeArgs": ["-r", "${workspaceFolder}/.pnp.cjs"],
      "console": "integratedTerminal",
      "program": "${workspaceFolder}/packages/cli/dist/cli.js",
      "args": ["extract", "/Users/nbudin/Downloads/Operation - Recon", "extracted/oprecon"],
      "preLaunchTask": "Build all packages",
      "outFiles": [
        "${workspaceFolder}/packages/cli/dist/**/*.js",
        "${workspaceFolder}/packages/cli/dist/**/*.js.map",
        "${workspaceFolder}/packages/core/dist/**/*.js",
        "${workspaceFolder}/packages/core/dist/**/*.js.map"
      ]
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Re-Extract",
      "skipFiles": ["<node_internals>/**"],
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["node"],
      "console": "integratedTerminal",
      "program": "${workspaceFolder}/packages/cli/dist/cli.js",
      "args": ["extract", "build/kq1", "reextracted/kq1"],
      "preLaunchTask": "Build all packages",
      "outFiles": [
        "${workspaceFolder}/packages/cli/dist/**/*.js",
        "${workspaceFolder}/packages/core/dist/**/*.js"
      ]
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch Build",
      "skipFiles": ["<node_internals>/**"],
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["node"],
      "console": "integratedTerminal",
      "program": "${workspaceFolder}/packages/cli/dist/cli.js",
      "args": ["build", "/Users/nbudin/src/agikit-project-template"],
      "preLaunchTask": "Build all packages",
      "outFiles": [
        "${workspaceFolder}/packages/cli/dist/**/*.js",
        "${workspaceFolder}/packages/core/dist/**/*.js"
      ]
    },
    {
      "name": "Run Extension",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "${workspaceFolder}/../agikit-project-template",
        "--extensionDevelopmentPath=${workspaceFolder}/packages/agikit-vscode"
      ],
      "outFiles": ["${workspaceFolder}/out/**/*.js"],
      "preLaunchTask": "Build agikit-vscode"
    },
    {
      "name": "Extension Tests",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/packages/agikit-vscode",
        "--extensionTestsPath=${workspaceFolder}/out/test/suite/index"
      ],
      "outFiles": ["${workspaceFolder}/out/test/**/*.js"],
      "preLaunchTask": "Build agikit-vscode"
    },
    {
      "type": "node",
      "name": "vscode-jest-tests.v2",
      "request": "launch",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "disableOptimisticBPs": true,
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "yarn",
      "args": [
        "run",
        "jest",
        "--runInBand",
        "--watchAll=false",
        "--testNamePattern",
        "${jest.testNamePattern}",
        "--runTestsByPath",
        "${jest.testFile}"
      ]
    }
  ]
}
