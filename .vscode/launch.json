{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch Extract",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "/Users/nbudin/.nodenv/versions/14.15.1/bin/node",
      "program": "${workspaceFolder}/packages/cli/dist/cli.js",
      "args": [
        "extract",
        "/Users/nbudin/Downloads/Space Quest 2",
        "extracted/sq2"
        // "/Users/nbudin/Downloads/King's Quest 1 (AGI, DOS)",
        // "extracted/kq1"
        // "/Users/nbudin/Downloads/AGI Contest 2 Template",
        // "extracted/contest2"
      ],
      "preLaunchTask": "npm: build - packages/cli",
      "outFiles": [
        "${workspaceFolder}/packages/cli/dist/**/*.js",
        "${workspaceFolder}/packages/agikit-core/dist/**/*.js"
      ]
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Re-Extract",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "/Users/nbudin/.nodenv/versions/14.15.1/bin/node",
      "program": "${workspaceFolder}/packages/cli/dist/cli.js",
      "args": [
        "extract",
        // "build/contest2",
        // "reextracted/contest2"
        "build/kq1",
        "reextracted/kq1"
      ],
      "preLaunchTask": "npm: build - packages/cli",
      "outFiles": [
        "${workspaceFolder}/packages/cli/dist/**/*.js",
        "${workspaceFolder}/packages/agikit-core/dist/**/*.js"
      ]
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch Build",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "/Users/nbudin/.nodenv/versions/14.15.1/bin/node",
      "program": "${workspaceFolder}/packages/cli/dist/cli.js",
      "args": [
        "build",
        // "/Users/nbudin/Downloads/AGI Contest 2 Template/src",
        "extracted/sq2",
        // "extracted/kq1",
      ],
      "preLaunchTask": "npm: build - packages/cli",
      "outFiles": [
        "${workspaceFolder}/packages/cli/dist/**/*.js",
        "${workspaceFolder}/packages/agikit-core/dist/**/*.js"
      ]
    },
    {
      "name": "Run Extension",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "${workspaceFolder}/../agikit-project-template",
        "--extensionDevelopmentPath=${workspaceFolder}/packages/agikit-vscode"
      ],
      "outFiles": [
        "${workspaceFolder}/out/**/*.js"
      ],
      "preLaunchTask": "Build agikit-vscode"
    },
    {
      "name": "Extension Tests",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/packages/agikit-vscode",
        "--extensionTestsPath=${workspaceFolder}/out/test/suite/index"
      ],
      "outFiles": [
        "${workspaceFolder}/out/test/**/*.js"
      ],
      "preLaunchTask": "Build agikit-vscode"
    }
  ]
}
