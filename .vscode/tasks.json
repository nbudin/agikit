{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Build agikit-vscode",
			"type": "npm",
			"script": "webpack",
			"problemMatcher": "$tsc-watch",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"options": {
				"cwd": "packages/agikit-vscode"
			}
		},
		{
			"label": "Watch agikit-vscode",
			"type": "npm",
			"script": "webpack-dev",
			"problemMatcher": "$tsc-watch",
			"isBackground": true,
			"options": {
				"cwd": "packages/agikit-vscode"
			}
		}
	]
}
